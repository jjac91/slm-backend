{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 1,
      "maxLength": 25
    },
    "password": {
      "type": "string",
      "minLength": 5,
      "maxLength": 20
    },
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50
    },
    "isAdmin": {
      "type": "boolean"
    }
  },
  "additionalProperties": false,
  "required": [
    "username",
    "password",
    "name"
  ]
}
